/interface vlan
add interface=wlan1 name=wlan.vlan3017 vlan-id=3017
/interface wireless
add disabled=no keepalive-frames=disabled mac-address=66:D1:54:D7:8C:23 master-interface=wlan1 multicast-buffering=disabled name=HS ssid=ARH-WIFI wds-cost-range=0 wds-default-cost=0 wps-mode=disabled
add disabled=no keepalive-frames=disabled mac-address=66:D1:54:D7:8C:24 master-interface=wlan1 multicast-buffering=disabled name="OFFICE wlan" security-profile=profile1 ssid=ARH-OFFICE vlan-id=3017 vlan-mode=use-tag wds-cost-range=0 wds-default-cost=0 wps-mode=disabled
/interface vlan
add interface="OFFICE wlan" name=wifi.vlan3017 vlan-id=3017
/ip pool
add name=pool-of ranges=10.147.29.105-10.147.29.199
add name=hs ranges=172.18.0.2-172.18.0.254
/ip dhcp-server
